@{
    ViewBag.Title = "My Favourties";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}
@model List<GDSite.Models.Favourty>
@if(Session["uID"]!=null)
{
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">

    <script src="~/Content/js/jquery.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <!-- Custom CSS -->
    <link href="~/Content/css/modern-business.css" rel="stylesheet">
    <!-- Custom Fonts -->
    <link href="~/Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <title>Designers</title>
</head>


<body>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                   My Favourtie Designers
                </h1>

            </div>
        </div>
        @foreach (GDSite.Models.Favourty v in Model)
        {
  
            <div class="row">
                <div class="col-md-7">
                    <a href="ViewList?id=@v.DAccount.Id">
                        <img class="img-responsive" src=@v.DAccount.image width="500" height="150" alt="">
                    </a>
                </div>
                <div class="col-md-5">
                    <h2>@v.DAccount.name</h2>
                    <p><font size="3">Type: @v.DAccount.type</font></p>
                    <p><font size="3">tel:+@v.DAccount.number</font></p>
                    <p><font size="3">Address: @v.DAccount.address</font></p>
                    </div>
            </div>
            <br /> <br />

        }
    </div>
</body>


</html>
    }
else
{
<h2>ONLY FOR AUTHORIZED USERS</h2>
}
